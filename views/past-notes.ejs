<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="styles.css">
  <title>Past Notes</title>
</head>
<body>
  <div class="navbar">
    <ul>
      <li><a href="/past-notes">Past Notes</a></li>
      <li><a href="/login">Login/Signup</a></li>
    </ul>
  </div>
  <h1>Past Notes</h1>
  <div id="notes-container">
    <ul>
      <% notes.forEach((note) => { %>
        <li>
          <a href="/note?id=<%= note.id %>"><%= note.patient_name %> - <%= note.timestamp %></a>
        </li>
      <% }); %>
    </ul>
  </div>
</body>
</html>
